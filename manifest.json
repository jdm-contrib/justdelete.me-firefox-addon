{
    "manifest_version": 2,
    "name": "JustDeleteMe",
    "version": "1.5.1",
    "icons": {
        "128": "res/ext_icon_144.png"
    },
    "browser_action": {
        "default_icon": {
            "144": "res/ext_icon_144.png"
        },
        "default_popup": "pub/index.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>",
        "webNavigation"
    ],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": true
    }
}
